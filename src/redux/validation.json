{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "PersonRole": {
      "type": "string",
      "enum": [
        "PRIMARY",
        "SPOUSE",
        "DEPENDENT",
        "EMPLOYER"
      ]
    },
    "Person": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "ssid": {
          "type": "string"
        },
        "role": {
          "$ref": "#/definitions/PersonRole"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "ssid",
        "role"
      ],
      "additionalProperties": false
    },
    "Dependent": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "ssid": {
          "type": "string"
        },
        "role": {
          "$ref": "#/definitions/PersonRole"
        },
        "relationship": {
          "type": "string"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "relationship",
        "role",
        "ssid"
      ],
      "additionalProperties": false
    },
    "Address": {
      "type": "object",
      "properties": {
        "address": {
          "type": "string"
        },
        "aptNo": {
          "type": "string"
        },
        "city": {
          "type": "string"
        },
        "state": {
          "type": "string"
        },
        "zip": {
          "type": "string"
        },
        "foreignCountry": {
          "type": "string"
        },
        "province": {
          "type": "string"
        },
        "postalCode": {
          "type": "string"
        }
      },
      "required": [
        "address",
        "city",
        "state",
        "zip"
      ],
      "additionalProperties": false
    },
    "PrimaryPerson": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "ssid": {
          "type": "string"
        },
        "role": {
          "$ref": "#/definitions/PersonRole"
        },
        "address": {
          "$ref": "#/definitions/Address"
        }
      },
      "required": [
        "address",
        "firstName",
        "lastName",
        "role",
        "ssid"
      ],
      "additionalProperties": false
    },
    "Employer": {
      "type": "object",
      "properties": {
        "EIN": {
          "type": "string"
        },
        "employerName": {
          "type": "string"
        },
        "address": {
          "$ref": "#/definitions/Address"
        }
      },
      "required": [
        "EIN",
        "employerName"
      ],
      "additionalProperties": false
    },
    "AccountType": {
      "type": "string",
      "enum": [
        "checking",
        "savings"
      ]
    },
    "Refund": {
      "type": "object",
      "properties": {
        "routingNumber": {
          "type": "string"
        },
        "accountNumber": {
          "type": "string"
        },
        "accountType": {
          "$ref": "#/definitions/AccountType"
        }
      },
      "required": [
        "routingNumber",
        "accountNumber",
        "accountType"
      ],
      "additionalProperties": false
    },
    "IncomeW2": {
      "type": "object",
      "properties": {
        "occupation": {
          "type": "string"
        },
        "income": {
          "type": "number"
        },
        "fedWithholding": {
          "type": "number"
        },
        "employer": {
          "$ref": "#/definitions/Employer"
        },
        "personRole": {
          "type": "string"
        }
      },
      "required": [
        "occupation",
        "income",
        "fedWithholding",
        "personRole"
      ],
      "additionalProperties": false
    },
    "FilingStatus": {
      "type": "string",
      "enum": [
        "S",
        "MFJ",
        "MFS",
        "HOH",
        "W"
      ]
    },
    "ContactInfo": {
      "type": "object",
      "properties": {
        "contactPhoneNumber": {
          "type": "string"
        },
        "contactEmail": {
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "TaxPayer": {
      "type": "object",
      "properties": {
        "contactPhoneNumber": {
          "type": "string"
        },
        "contactEmail": {
          "type": "string"
        },
        "filingStatus": {
          "$ref": "#/definitions/FilingStatus"
        },
        "primaryPerson": {
          "$ref": "#/definitions/PrimaryPerson"
        },
        "spouse": {
          "$ref": "#/definitions/Person"
        },
        "dependents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Dependent"
          }
        }
      },
      "required": [
        "dependents"
      ],
      "additionalProperties": false
    },
    "Information": {
      "type": "object",
      "properties": {
        "w2s": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/IncomeW2"
          }
        },
        "refund": {
          "$ref": "#/definitions/Refund"
        },
        "taxPayer": {
          "$ref": "#/definitions/TaxPayer"
        }
      },
      "required": [
        "w2s",
        "taxPayer"
      ],
      "additionalProperties": false
    },
    "TaxesState": {
      "type": "object",
      "properties": {
        "information": {
          "$ref": "#/definitions/Information"
        }
      },
      "required": [
        "information"
      ],
      "additionalProperties": false
    }
  }
}