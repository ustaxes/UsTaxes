        "type": "string",
        "required": false
      },
      "paymentMethod": {
        "field": "paymentMethod",
        "label": "Payment method",
        "type": "enum",
        "required": false,
        "values": ["cash", "credit", "debit", "check", "other"]
      }
    }
  },
  "validationRules": {
    "ssn": {
      "pattern": "^\\d{3}-\\d{2}-\\d{4}$",
      "length": 11,
      "description": "Format: XXX-XX-XXXX"
    },
    "ein": {
      "pattern": "^\\d{2}-\\d{7}$",
      "length": 10,
      "description": "Format: XX-XXXXXXX"
    },
    "stateCode": {
      "pattern": "^[A-Z]{2}$",
      "length": 2,
      "validCodes": [
        "AL", "AK", "AZ", "AR", "CA", "CO", "CT", "DE", "FL", "GA",
        "HI", "ID", "IL", "IN", "IA", "KS", "KY", "LA", "ME", "MD",
        "MA", "MI", "MN", "MS", "MO", "MT", "NE", "NV", "NH", "NJ",
        "NM", "NY", "NC", "ND", "OH", "OK", "OR", "PA", "RI", "SC",
        "SD", "TN", "TX", "UT", "VT", "VA", "WA", "WV", "WI", "WY", "DC"
      ]
    },
    "zip": {
      "pattern": "^\\d{5}(-\\d{4})?$",
      "description": "Format: XXXXX or XXXXX-XXXX"
    },
    "currency": {
      "pattern": "^-?\\d+(\\.\\d{1,2})?$",
      "description": "Numeric value with up to 2 decimal places"
    },
    "date": {
      "pattern": "^(0[1-9]|1[0-2])/(0[1-9]|[12]\\d|3[01])/(19|20)\\d{2}$",
      "description": "Format: MM/DD/YYYY"
    }
  }
