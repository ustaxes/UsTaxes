  "RECEIPT": {
    "formName": "Business Receipt",
    "taxYears": [2019, 2020, 2021, 2022, 2023, 2024],
    "fields": {
      "date": {
        "field": "date",
        "label": "Date of expense",
        "type": "date",
        "required": true,
        "format": "MM/DD/YYYY"
      },
      "vendor": {
        "field": "vendor",
        "label": "Vendor/merchant name",
        "type": "string",
        "required": true
      },
      "amount": {
        "field": "amount",
        "label": "Total amount",
        "type": "currency",
        "required": true
      },
      "category": {
        "field": "category",
        "label": "Expense category",
        "type": "enum",
        "required": true,
        "values": [
          "meals",
          "travel",
          "supplies",
          "equipment",
          "utilities",
          "advertising",
          "professional-services",
          "rent",
          "insurance",
          "other"
