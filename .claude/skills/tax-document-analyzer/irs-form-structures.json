      "1": {
        "field": "income",
        "label": "Wages, tips, other compensation",
        "type": "currency",
        "required": true,
        "box": "1"
      },
      "2": {
        "field": "fedWithholding",
        "label": "Federal income tax withheld",
        "type": "currency",
        "required": true,
        "box": "2"
      },
      "3": {
        "field": "ssWages",
        "label": "Social security wages",
        "type": "currency",
        "required": true,
        "box": "3"
      },
      "4": {
        "field": "ssWithholding",
        "label": "Social security tax withheld",
        "type": "currency",
        "required": true,
        "box": "4"
      },
      "5": {
        "field": "medicareIncome",
        "label": "Medicare wages and tips",
        "type": "currency",
        "required": true,
        "box": "5"
      },
      "6": {
        "field": "medicareWithholding",
        "label": "Medicare tax withheld",
        "type": "currency",
        "required": true,
        "box": "6"
      },
      "7": {
        "field": "socialSecurityTips",
        "label": "Social security tips",
        "type": "currency",
        "required": false,
        "box": "7"
      },
      "8": {
        "field": "allocatedTips",
        "label": "Allocated tips",
        "type": "currency",
        "required": false,
        "box": "8"
      },
      "10": {
        "field": "dependentCareBenefits",
        "label": "Dependent care benefits",
        "type": "currency",
        "required": false,
        "box": "10"
      },
      "12": {
        "field": "box12",
        "label": "Various codes",
        "type": "array",
        "required": false,
        "box": "12",
        "codes": {
          "A": "Uncollected social security or RRTA tax on tips",
          "B": "Uncollected Medicare tax on tips",
          "C": "Taxable cost of group-term life insurance over $50,000",
          "D": "Elective deferrals to a 401(k), 403(b), or 457(b) plan",
          "DD": "Cost of employer-sponsored health coverage",
          "E": "Elective deferrals under a 403(b) salary reduction agreement",
          "F": "Elective deferrals under a 408(k)(6) salary reduction SEP",
          "G": "Elective deferrals and employer contributions to a 457(b) plan",
          "H": "Elective deferrals to a 501(c)(18)(D) tax-exempt organization plan",
          "W": "Employer contributions to HSA",
          "Y": "Deferrals under a 409A nonqualified deferred compensation plan",
          "Z": "Income under a 409A nonqualified deferred compensation plan"
        }
      },
      "13": {
        "field": "checkboxes",
        "label": "Statutory employee, Retirement plan, Third-party sick pay",
        "type": "checkboxes",
        "required": false,
        "box": "13"
      },
      "15": {
        "field": "state",
        "label": "State",
        "type": "stateCode",
        "required": false,
        "box": "15"
      },
      "16": {
        "field": "stateWages",
        "label": "State wages, tips, etc.",
        "type": "currency",
        "required": false,
        "box": "16"
      },
      "17": {
        "field": "stateWithholding",
        "label": "State income tax",
        "type": "currency",
        "required": false,
        "box": "17"
      }
    },
    "additionalFields": {
      "occupation": {
        "type": "string",
        "required": false,
        "description": "Job title or occupation (not on form, may need to ask)"
      },
      "personRole": {
        "type": "enum",
        "required": true,
        "values": ["PRIMARY", "SPOUSE"],
        "description": "Whether this W-2 is for primary taxpayer or spouse"
      }
    }
